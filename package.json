{
  "name": "visiosto.github.io",
  "private": true,
  "version": "0.1.0-dev",
  "description": "The shared source for the website of Visiosto",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "check-all": "npm-run-all format --parallel lint:scss lint:js",
    "ci-check": "npm-run-all nit --parallel lint:scss lint:js",
    "dev": "gatsby develop",
    "format": "prettier --config .prettierrc.json --write \"**/*.{js,jsx,json,md}\"",
    "nit": "prettier --config .prettierrc.json --list-different \"**/*.{js,jsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:js": "eslint 'src/**/*.js' 'src/**/*.jsx'",
    "clean": "gatsby clean",
    "start": "yarn dev",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visiosto/visiosto.github.io.git"
  },
  "author": "Antti Kivi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visiosto/visiosto.github.io/issues"
  },
  "homepage": "https://github.com/visiosto/visiosto.github.io#readme",
  "dependencies": {
    "gatsby": "^2.26.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-plugin-react": "^7.21.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "stylelint": "^13.8.0"
  }
}
