{
  "name": "visiosto-website",
  "private": true,
  "version": "0.31.0-dev",
  "description": "The website of Visiosto",
  "scripts": {
    "build": "gatsby build",
    "check-all": "npm-run-all format --parallel lint:js lint:css",
    "ci-check": "npm-run-all nit --parallel lint:js lint:css",
    "dev": "gatsby develop",
    "format": "prettier --config .prettierrc.json --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "nit": "prettier --config .prettierrc.json --list-different \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:css": "stylelint './src/**/*{.js,.jsx,.ts,.tsx}'",
    "type-check": "tsc",
    "clean": "gatsby clean",
    "start": "yarn dev",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visiosto/visiosto-website.git"
  },
  "author": "Antti Kivi <antti.kivi@visiosto.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visiosto/visiosto-website/issues"
  },
  "homepage": "https://github.com/visiosto/visiosto-website#readme",
  "dependencies": {
    "@reach/router": "^1.3.4",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "gatsby": "^3.4.2",
    "gatsby-plugin-catch-links": "^3.0.0",
    "gatsby-plugin-google-gtag": "^3.3.0",
    "gatsby-plugin-google-tagmanager": "^3.4.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-manifest": "^3.4.0",
    "gatsby-plugin-netlify": "^3.1.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.2.1",
    "gatsby-plugin-sitemap": "^4.0.0",
    "gatsby-plugin-styled-components": "^4.3.0",
    "gatsby-source-contentful": "^5.2.1",
    "gatsby-source-filesystem": "^3.4.0",
    "gatsby-transformer-json": "^3.3.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.4.0",
    "js-search": "^2.0.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.17.1",
    "react-is": "^17.0.1",
    "scroll-to-element": "^2.0.3",
    "styled-components": "^5.2.3",
    "terser": "4.8.0",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.5",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.3.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^27.2.1",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-gatsby": "^1.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.13.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.2.4"
  }
}
