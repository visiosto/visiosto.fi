{
  "name": "visiosto.github.io",
  "private": true,
  "version": "0.5.0-dev",
  "description": "The shared source for the website of Visiosto",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "check-all": "npm-run-all format --parallel lint",
    "ci-check": "npm-run-all nit --parallel lint",
    "dev": "gatsby develop",
    "format": "prettier --config .prettierrc.json --write \"**/*.{js,jsx,json,md}\"",
    "nit": "prettier --config .prettierrc.json --list-different \"**/*.{js,jsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "lint": "eslint .",
    "clean": "gatsby clean && rm -rf src/__generated__",
    "start": "yarn dev",
    "serve": "gatsby serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/visiosto/visiosto.github.io.git"
  },
  "author": "Antti Kivi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/visiosto/visiosto.github.io/issues"
  },
  "homepage": "https://github.com/visiosto/visiosto.github.io#readme",
  "dependencies": {
    "@primer/octicons-react": "^11.2.0",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-i18n": "^1.0.1",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-sharp": "^2.13.4",
    "gatsby-plugin-styled-components": "^3.9.0",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-transformer-sharp": "^2.11.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.10.17",
    "react-is": "^17.0.1",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "eslint": "^7.13.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "stylelint": "^13.8.0"
  }
}
